{
  "parser": "babel-eslint",
  "extends": ["standard"],
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/jsx-no-undef": 2,
    "react/wrap-multilines": 2
  },
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "classes": true,
    "jsx": true,
    "modules": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true,
    "browser": true
  }
}
